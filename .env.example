# GitHub token for API access
GITHUB_TOKEN=your_github_token_here

# Claude API key for code generation
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Helicone API key (for users to add to their projects)
HELICONE_API_KEY=your_helicone_api_key_here

# Environment: development, staging, or production
NODE_ENV=development

# Temporal Configuration
# For local development, use localhost:7233
# For Temporal Cloud, use your regional endpoint
TEMPORAL_ADDRESS=localhost:7233

# Temporal namespace
# For local: default
# For Temporal Cloud: your-namespace.account-id
TEMPORAL_NAMESPACE=default

# Temporal Cloud API Key (only needed for cloud)
# TEMPORAL_API_KEY=your_temporal_api_key_here